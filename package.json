{
  "name": "ng-tree-antd",
  "version": "1.0.1",
  "main": "index.js",
  "typings": "index.d.ts",
  "description": "A antd style of based on angular-tree-component.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cipchk/ng-tree-antd.git"
  },
  "keywords": [
    "ng-tree-antd",
    "angular-tree",
    "angular4-tree",
    "ng2-tree",
    "ng4-tree",
    "ng-antd-tree",
    "antd-tree",
    "tree"
  ],
  "author": "cipchk <cipchk>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cipchk/ng-tree-antd/issues"
  },
  "homepage": "https://github.com/cipchk/ng-tree-antd#readme",
  "scripts": {
    "demo.serve": "run-s build link demo.build",
    "demo.gh-pages": "run-s build demo.build demo.deploy",
    "demo.build": "ng build -prod",
    "demo.deploy": "gh-pages -d demo/dist",
    "link": "ngm link -p src --here",
    "lint": "exit 0",
    "build": "ngm build -p src --clean",
    "build.watch": "ngm build -p src --watch --skip-bundles",
    "pretest": "run-s lint build link",
    "test": "ng test -sr",
    "test-coverage": "ng test -sr -cc"
  },
  "dependencies": {
    "angular-tree-component": "^5.0.0"
  },
  "peerDependencies": {},
  "devDependencies": {
    "@angular/animations": "~4.4.6",
    "@angular/cli": "~1.4.7",
    "@angular/common": "~4.4.6",
    "@angular/compiler": "~4.4.6",
    "@angular/compiler-cli": "~4.4.6",
    "@angular/core": "~4.4.6",
    "@angular/forms": "~4.4.6",
    "@angular/http": "~4.4.6",
    "@angular/language-service": "~4.4.6",
    "@angular/platform-browser": "~4.4.6",
    "@angular/platform-browser-dynamic": "~4.4.6",
    "@angular/router": "~4.4.6",
    "@types/jasmine": "~2.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~6.0.60",
    "@types/webpack": "^3.0.14",
    "codelyzer": "~3.2.0",
    "core-js": "^2.4.1",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-zorro-antd": "^0.6.0-rc.3",
    "ngm-cli": "^0.8.3",
    "npm-run-all": "^4.1.1",
    "protractor": "~5.1.2",
    "rxjs": "^5.4.3",
    "ts-node": "~3.2.0",
    "tslint": "~5.7.0",
    "typescript": "~2.3.3",
    "zone.js": "^0.8.14"
  }
